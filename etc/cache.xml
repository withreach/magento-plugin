<?xml version="1.0"?>
<!--
This file is needed to show this custom cache option on Admin panel.
When it is both visible on the Admin panel and enabled then a merchant/admin with enough privilege 
can flush or clean it from there.
-->
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Cache/etc/cache.xsd">
    <type name="reach_payment_custom_cache" instance="Reach\Payment\Helper\Cacher">
        <label>Reach Custom Cache</label>
        <description>Reach Custom Cache to Store Currency Precision</description>
    </type>
</config>
